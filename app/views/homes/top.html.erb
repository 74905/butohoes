<div class="animsition-overlay" data-animsition-overlay="true">
  <main>
    <div class="main_visual">
      <span class="visual-text">1st EP</span>
      <span class="visual-text_main">Lost in Watercycle</span>
      <span class="visual-text_create">2021 5 anonymous</span>
      <a class="EP_link"><span>Coming Soon</span><em></em></a>
    </div>
    <div class="main">
    <div class="news-title">
      <span class="text">News</span>
    </div>
    <div class="container">
      <div class="row p-4 news-main">
        <% @news.each do |news| %>
          <div class="news-object col-lg-4 col-xs-12">
          <%= link_to news_path(news.id) do %>
            <div class="card bg-light h-100" style="width: 15rem; margin: 0 auto;">
                  <%= attachment_image_tag news, :news_image,ã€€format: 'png', fallback: "news.jpg", size: "237x200" %>
                <div class="card-body">
                  <p class="card-text"><%= news.title%></p>
                  <p class="card-text"><small class="text-muted"><%= news.created_at.strftime('%Y-%m-%d') %></small></p>
                </div>
            </div>
          <% end %>
          </div>
        <% end %>
      </div>
    </div>
        <div class="more-news">
        <%=link_to news_index_path, class:'bt3 animsition-link', data: {"turbolinks" => false} do %>
          Learn More
          <span class='line-1'></span>
          <span class='line-2'></span>
          <span class='line-3'></span>
          <span class='line-4'></span>
        <% end %>
        </div>
    <div class="container">
    <div class="row">
      <div class="col-lg-6">
        <div class="twitter">
              <a class="twitter-timeline" data-lang="ja" data-width="300" data-height="400" data-chrome="noheader nofooter" href="https://twitter.com/butohes_japan?ref_src=twsrc%5Etfw">Tweets by butohes_japan</a>
              <script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>
        </div>
    </div>
    </div>
  </div>
