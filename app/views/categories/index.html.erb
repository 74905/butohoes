<%= form_with model:@category, local:true do |f| %>
<%= f.label "カテゴリー"%>
<%= f.text_field :name, class:"form-control"%>
<%= f.submit "追加" %>
<% end %>

<% @categories.each do |category|%>
<%= category.name %>
<% end %>
<p>TOP MUSIC</p>
<%= form_with model: @top_music, local:true do |f| %>
   <div class="category">
     <%= f.collection_select(:music_id, Music.all, :id, :title, prompt: "-- 選択してください--") %>
   </div>
  <%= f.submit '反映' %>
<% end %>